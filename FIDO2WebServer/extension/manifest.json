{
    "manifest_version": 3,
    "name": "Software FIDO2 Authenticator",
    "version": "1.0",
    "description": "Emulates a FIDO2 authenticator in JS",
    "permissions": [
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_start"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["injected.js","lib/fido2-lib.min.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }